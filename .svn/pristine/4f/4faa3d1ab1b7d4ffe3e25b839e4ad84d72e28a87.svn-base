	<delete id="delSkill" parameterType="kr.or.ddit.common.mypage.memInfo.vo.GenSkillVO">
		DELETE FROM GEN_SKILL
		WHERE
		SKILL_CODE=#{skillCode}
		AND
		GEN_ID = #{genId}
	</delete>
	


	<select id="workType" resultType="kr.or.ddit.common.vo.CommonCodeVO">
		SELECT COMMON_CODE.CODE_ID, COMMON_CODE.CODE_NAME, COMMON_CODE.TYPE_ID, COMMON_CODETYPE.TYPE_NAME
		  FROM COMMON_CODE 
		  INNER JOIN COMMON_CODETYPE ON (COMMON_CODE.TYPE_ID = COMMON_CODETYPE.TYPE_ID)
